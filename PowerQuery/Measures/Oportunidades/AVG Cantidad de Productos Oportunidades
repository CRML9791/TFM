AVG Cantidad de Productos Oportunidades = 
VAR CANT =
    SUMX (
        SUMMARIZE (
            'Facts Oportunidad',
            'Facts Oportunidad'[opportunityid],
            'Facts Oportunidad'[Cantidad]
        ),
        'Facts Oportunidad'[Cantidad]
    )
RETURN
    DIVIDE ( CANT, [NÂº Total Oportunidades] )
